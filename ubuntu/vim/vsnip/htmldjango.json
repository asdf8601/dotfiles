            "{% endblocktrans %}"
        ]
    },

    "blocktrans_with": {
        "description": "{% blocktrans as %}{% endblocktrans %}",
        "prefix": "blocktrans_with",
        "body": "{% blocktrans with $1=$2 ${3:trimmed }%}$0{% endblocktrans %}"
