        "body": "{% blocktrans %}$0{% endblocktrans %}"
    },

    "blocktrans_paste": {
        "description": "{% blocktrans %}(paste){% endblocktrans %}",
        "prefix": "blocktrans_paste",
        "body": [
            "{% blocktrans ${2:trimmed }%}",
