        "prefix": "autoescape",
        "body": "{% autoescape ${1|off,on|} %}$0{% endautoescape %}"
    },

    "autoescape_paste": {
        "description": "{% autoescape %} (paste)",
        "prefix": "autoescape_paste",
        "body": [
            "{% autoescape ${1|off,on|} %}",
            "$0${CLIPBOARD}",
